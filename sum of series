
class Solution:
    def seriesSum(self, n : int) -> int:
        # #code here
           i=1
        sum=0
        while(i<=n):
            sum=sum+i
            i=i+1
        return sum
#{ 
 # Driver Code Starts
if __name__ == '__main__':
    t = int(input())
    for _ in range(t):

        n = int(input())

        obj = Solution()
        res = obj.seriesSum(n)

        print(res)

# } Driver Code Ends
