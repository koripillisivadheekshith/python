# Return true if str is binary, else false
def isBinary(str):
    #code here
    #approach -- 1
    # s = list(set(str))
    # sum = 0
    # for i in s:
    #     sum += int(i)
    # if sum > 1:
    #     return False
    # return True

 


    # Approach - 2
    for i in str:
        if i != '0' and i != '1':
            return False
    return True


